{
  "expo": {
    "name": "Our House",
    "slug": "ourhouse",
    "version": "4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ourhouse.org"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "ourhouse.org",
      "versionCode": 4,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "XXXX"
        }
      }
    }
  }
}
