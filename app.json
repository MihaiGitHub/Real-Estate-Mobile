{
  "expo": {
    "name": "Century Homes",
    "slug": "century-homes",
    "version": "2.1.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "SENTRY_ORG",
            "project": "CENTURY-HOMES"
          }
        }
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "XXXX"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.century.homes",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "XXXX"
        }
      },
      "package": "com.century.homes",
      "versionCode": 136,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "XXXX"
      }
    }
  }
}
